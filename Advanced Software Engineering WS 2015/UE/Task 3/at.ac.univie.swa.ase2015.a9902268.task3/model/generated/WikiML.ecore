<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="wikiML" nsURI="http://www.ac.at/univie/swa/ase2015/a9902268/task3/WikiML"
    nsPrefix="wikiML">
  <eClassifiers xsi:type="ecore:EClass" name="WikiMLPage">
    <eStructuralFeatures xsi:type="ecore:EReference" name="title" upperBound="-1"
        eType="#//PageTitle" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="section" upperBound="-1"
        eType="#//AbstractSection" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PageTitle">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="content" upperBound="-1"
        eType="#//AbstractParagraph" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractSection">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="content" upperBound="-1"
        eType="#//AbstractParagraph" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SubSection" eSuperTypes="#//AbstractSection"/>
  <eClassifiers xsi:type="ecore:EClass" name="SubSubSection" eSuperTypes="#//AbstractSection"/>
  <eClassifiers xsi:type="ecore:EClass" name="SubSubSubSection" eSuperTypes="#//AbstractSection"/>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractParagraph"/>
  <eClassifiers xsi:type="ecore:EClass" name="NormalParagraph" eSuperTypes="#//AbstractParagraph #//BoldParagraph #//ItalicParagraph">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BoldParagraph" eSuperTypes="#//AbstractParagraph"/>
  <eClassifiers xsi:type="ecore:EClass" name="ItalicParagraph" eSuperTypes="#//AbstractParagraph"/>
</ecore:EPackage>
