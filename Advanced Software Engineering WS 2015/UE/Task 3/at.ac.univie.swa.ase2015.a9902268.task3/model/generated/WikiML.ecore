<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="wikiML" nsURI="http://www.ac.at/univie/swa/ase2015/a9902268/task3/WikiML"
    nsPrefix="wikiML">
  <eClassifiers xsi:type="ecore:EClass" name="wikipage">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="title" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="content" upperBound="-1"
        eType="#//InlineContent" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sections" upperBound="-1"
        eType="#//AbstractSection" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractSection">
    <eStructuralFeatures xsi:type="ecore:EReference" name="name" eType="#//Text" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="content" upperBound="-1"
        eType="#//InlineContent" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SubsectionLevel1" eSuperTypes="#//AbstractSection"/>
  <eClassifiers xsi:type="ecore:EClass" name="SubsectionLevel2" eSuperTypes="#//AbstractSection"/>
  <eClassifiers xsi:type="ecore:EClass" name="SubsectionLevel3" eSuperTypes="#//AbstractSection"/>
  <eClassifiers xsi:type="ecore:EClass" name="SubsectionLevel4" eSuperTypes="#//AbstractSection"/>
  <eClassifiers xsi:type="ecore:EClass" name="InlineContent"/>
  <eClassifiers xsi:type="ecore:EClass" name="FormattedContent" eSuperTypes="#//InlineContent"/>
  <eClassifiers xsi:type="ecore:EClass" name="ItalicContent" eSuperTypes="#//FormattedContent"/>
  <eClassifiers xsi:type="ecore:EClass" name="BoldContent" eSuperTypes="#//FormattedContent"/>
  <eClassifiers xsi:type="ecore:EClass" name="BoldItalicContent" eSuperTypes="#//FormattedContent"/>
  <eClassifiers xsi:type="ecore:EClass" name="Text" eSuperTypes="#//InlineContent #//ItalicContent #//BoldContent #//BoldItalicContent"/>
  <eClassifiers xsi:type="ecore:EClass" name="UnFormattedText" eSuperTypes="#//Text">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="words" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Hyperlink" eSuperTypes="#//Text"/>
  <eClassifiers xsi:type="ecore:EClass" name="ExternalLink" eSuperTypes="#//Hyperlink">
    <eStructuralFeatures xsi:type="ecore:EReference" name="pageName" eType="#//UnFormattedText"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ExternalLinkAlternative" eSuperTypes="#//Hyperlink">
    <eStructuralFeatures xsi:type="ecore:EReference" name="url" eType="#//UnFormattedText"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="alternativeText" eType="#//UnFormattedText"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InternalLink" eSuperTypes="#//Hyperlink">
    <eStructuralFeatures xsi:type="ecore:EReference" name="pageName" eType="#//UnFormattedText"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InternalLinkAlternative" eSuperTypes="#//Hyperlink">
    <eStructuralFeatures xsi:type="ecore:EReference" name="pageName" eType="#//UnFormattedText"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="alternativeText" eType="#//UnFormattedText"
        containment="true"/>
  </eClassifiers>
</ecore:EPackage>
